{
  "name": "@sanity/next-studio-layout",
  "version": "1.0.0",
  "description": "Toolkit for using Sanity.io Studio on a Next.js page",
  "keywords": [
    "sanity",
    "sanity.io",
    "next.js",
    "studio",
    "studio-v3"
  ],
  "homepage": "https://github.com/sanity-io/next-studio-layout#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/next-studio-layout/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/next-studio-layout.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "source": "./src/index.ts",
  "types": "./lib/types/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "next build",
    "clean": "rimraf lib",
    "dev": "next",
    "format": "npx prettier --write . --ignore-path .gitignore && npx eslint . --fix",
    "lint": "next lint",
    "prepublishOnly": "npm run clean && parcel build",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/styled-components": "^5.1.25",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "next": "^12.2.4",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^17",
    "react-dom": "^17",
    "react-icons": "^3.11.0",
    "sanity": "^3.0.0-dev-preview.12",
    "semantic-release": "^19.0.3",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "next": "^12.2",
    "react": "^17 || ^18.0.0",
    "sanity": "dev-preview",
    "styled-components": "^5.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
