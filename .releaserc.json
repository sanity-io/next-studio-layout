{
  "branches": [
    { "name": "studio-v2", "channel": "latest" },
    { "name": "main", "channel": "studio-v3", "prerelease": "v3-studio" }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/npm",
      {
        "tarballDir": "release"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": "release/*.tgz"
      }
    ]
  ],
  "preset": "angular"
}
